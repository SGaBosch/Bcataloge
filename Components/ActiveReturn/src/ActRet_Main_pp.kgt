   ram_groesse xActRet_NomSteerSpdFrac_xdu16
   {
      umrechnung     RIGHTSH16NP;
      codesyntax     csyGLRAMuw;
      datentyp       uw;
      minp_w         0.0;
      max_w          65535; // => 1.0 - 1 digit
      init_wertp     {0.0};
   }
