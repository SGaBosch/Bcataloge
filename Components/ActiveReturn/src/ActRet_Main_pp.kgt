   {
      deutsch  "difference of steering speed controller";
      englisch "difference of steering speed controller";
   }
// -----------------------------------------------------------------------------
   ram_groesse xActRet_TorqueFactorFilt_xds32
   {
      umrechnung     RIGHTSH20NP;
