      englisch "value of xActRet_MotorTorqueGain_XAU16";
   }
// -----------------------------------------------------------------------------
   ram_groesse xActRet_SpeedFactorFilt_xds32
   {
      umrechnung     RIGHTSH20NP;
      codesyntax     csyGLRAMsl;
