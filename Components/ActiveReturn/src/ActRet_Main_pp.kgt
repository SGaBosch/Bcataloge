// RAM-Groessen
// -----------------------------------------------------------------------------
   ram_groesse yActRet_NomSteerSpdIdx_xdu8
   {
      umrechnung     ZU0;
      codesyntax     csyGLRAMub;
      datentyp       ub;
      minp_w         0.0;
      maxp_w         254.0; // max value needs to be equal (or more) than the char field; doesn't care
      init_wertp     {0.0};
   }
